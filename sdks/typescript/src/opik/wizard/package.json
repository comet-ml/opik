{
  "name": "opik-wizard",
  "version": "0.1.0",
  "homepage": "https://github.com/comet-ml/opik",
  "repository": "https://github.com/comet-ml/opik",
  "description": "The Opik wizard helps you to configure your Node.js project with Opik SDK",
  "keywords": [
    "opik",
    "wizard",
    "sdk",
    "cli",
    "nodejs",
    "setup",
    "install",
    "configure",
    "tracing",
    "llm"
  ],
  "bin": {
    "opik-wizard": "dist/bin.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/bin.*",
    "dist/src",
    "dist/templates",
    "package.json",
    "README.md"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@clack/core": "^0.3.4",
    "@clack/prompts": "0.7.0",
    "axios": "1.7.4",
    "chalk": "^2.4.1",
    "fast-glob": "^3.3.3",
    "glob": "9.3.5",
    "inquirer": "^6.2.0",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "magicast": "^0.2.10",
    "opn": "^5.4.0",
    "posthog-node": "^4.3.1",
    "read-env": "^1.3.0",
    "recast": "^0.23.3",
    "semver": "^7.5.3",
    "uuid": "^11.1.0",
    "yargs": "^16.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/types": "~7.21.4",
    "@eslint/js": "^9.20.0",
    "@langchain/core": "^0.3.78",
    "@types/glob": "^7.2.0",
    "@types/inquirer": "^0.0.43",
    "@types/lodash": "^4.17.15",
    "@types/node": "^18.19.76",
    "@types/opn": "5.1.0",
    "@types/semver": "^7.3.7",
    "@types/yargs": "^16.0.9",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.0",
    "globals": "^15.14.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.24.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=3.10.7"
  },
  "packageManager": "pnpm@9.15.5+sha256.8472168c3e1fd0bff287e694b053fccbbf20579a3ff9526b6333beab8df65a8d",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "pnpm clean",
    "build:watch": "pnpm tsc -w",
    "build": "pnpm tsc",
    "postbuild": "chmod +x ./dist/bin.js && cp -r src/utils/rules dist/src/utils && cp -r templates dist/",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint:eslint": "eslint '**/*.{ts,tsx}'",
    "fix": "pnpm fix:eslint && pnpm fix:prettier",
    "fix:prettier": "prettier --write \"src/**/*.ts\"",
    "fix:eslint": "eslint '**/*.{ts,tsx}' --fix",
    "try": "tsx bin.ts"
  },
  "author": "Opik",
  "license": "Apache-2.0",
  "volta": {
    "node": "20.18.2",
    "pnpm": "9.15.5"
  }
}
