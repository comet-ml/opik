{
  "name": "opik-ts",
  "version": "0.1.3",
  "homepage": "https://github.com/comet-ml/opik",
  "repository": "https://github.com/comet-ml/opik",
  "description": "The Opik configure tool helps you to configure your Node.js project with Opik SDK",
  "keywords": [
    "opik",
    "sdk",
    "cli",
    "nodejs",
    "setup",
    "install",
    "configure",
    "tracing",
    "llm"
  ],
  "bin": {
    "opik-ts": "dist/bin.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/bin.*",
    "dist/src",
    "package.json",
    "README.md"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "@clack/core": "^0.5.0",
    "@clack/prompts": "0.11.0",
    "@langchain/core": "^1.1.15",
    "axios": "1.13.2",
    "chalk": "^5.6.2",
    "fast-glob": "^3.3.3",
    "glob": "13.0.0",
    "inquirer": "^13.2.0",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "magicast": "^0.5.1",
    "opn": "^6.0.0",
    "posthog-node": "^5.21.0",
    "read-env": "^2.0.0",
    "recast": "^0.23.11",
    "semver": "^7.7.3",
    "uuid": "^13.0.0",
    "yargs": "^18.0.0",
    "zod": "^3.25.55",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@babel/types": "~7.28.6",
    "@eslint/js": "^9.39.2",
    "@types/glob": "^9.0.0",
    "@types/inquirer": "^9.0.9",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.9",
    "@types/opn": "5.5.0",
    "@types/semver": "^7.7.1",
    "@types/yargs": "^17.0.35",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=3.10.7"
  },
  "packageManager": "pnpm@9.15.5+sha256.8472168c3e1fd0bff287e694b053fccbbf20579a3ff9526b6333beab8df65a8d",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "pnpm clean",
    "build:watch": "pnpm tsc -w",
    "build": "pnpm tsc",
    "postbuild": "chmod +x ./dist/bin.js && cp -r src/utils/rules dist/src/utils",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "lint:eslint": "eslint '**/*.{ts,tsx}'",
    "fix": "pnpm fix:eslint && pnpm fix:prettier",
    "fix:prettier": "prettier --write \"src/**/*.ts\"",
    "fix:eslint": "eslint '**/*.{ts,tsx}' --fix",
    "try": "tsx bin.ts",
    "typecheck": "tsc --noEmit"
  },
  "author": "Opik",
  "license": "Apache-2.0",
  "volta": {
    "node": "20.18.2",
    "pnpm": "9.15.5"
  }
}
